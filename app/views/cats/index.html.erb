<h1>Welcome <%= @user.user_name %>!</h1>
<form method="post" action=<%= session_url %>>
  <input type="hidden" name="_method" value="DELETE">
  <input type="submit" value="Log Out">
  <input type="hidden" name="authenticity_token" value=<%= form_authenticity_token %>>
</form>
<h2>Choose from all our different cats!</h2>
<table align = "center">
  <tr>
    <th>Cat Serial Number</th>
    <th>Name</th>
    <th>Age</th>
    <th>Birthdate</th>
    <th>Color</th>
    <th>Sex</th>
    <th>Link to Cat's profile page</th>
  </tr>
  <%@cats.each do |cat|%>
  <tr>
    <td><%=cat.id%></td>
    <td><%=cat.name%></td>
    <td><%=cat.age%></td>
    <td><%=cat.birth_date%></td>
    <td><%=cat.color%></td>
    <td><%=cat.sex%></td>
    <td><%= link_to "#{cat.name}'s profile", cat_url(cat)%></td>
  </tr>
  <%end%>
</table>
<br>
<%= link_to "Post a new cat!", new_cat_url %>
