<h1>Welcome <%= @user.user_name %>!</h1>

<table align="center">
<tr>
<td>
<form method="get" action=<%= cats_url %>>
  <input type="submit" value="Account Page">
</form>
</td><td>
<form method="post" action=<%= session_url %>>
  <input type="hidden" name="_method" value="DELETE">
  <input type="submit" value="Log Out">
  <input type="hidden" name="authenticity_token" value=<%= form_authenticity_token %>>
</form>
</td></tr>
</table>

<h2>Choose From All Our Different Cats!</h2>
<table align="center">
  <tr class="spaceUnder">
    <th>Serial Number</th>
    <th>Name</th>
    <th>Age</th>
    <th>Birthdate</th>
    <th>Color</th>
    <th>Sex</th>
    <th>Profile Page</th>
  </tr>
  <tr>
  <%@cats.each do |cat|%>
  <tr class="spaceUnder">
    <td><%=cat.id%></td>
    <td><%=cat.name%></td>
    <td><%=cat.age%></td>
    <td><%=cat.birth_date%></td>
    <td><%=cat.color%></td>
    <td><%=cat.sex%></td>
    <td><%= link_to "#{cat.name}'s profile", cat_url(cat)%></td>
  </tr>
  <%end%>
</table>
<br>
<%= link_to "Post A New Cat!", new_cat_url %>
